  <script>
    function showTime() {
      const date = new Date();
      const h = date.getHours(); 
      const m = date.getMinutes(); 
      const s = date.getSeconds(); 
      const time = h + ":" + m + ":" + s;
      document.getElementById("MyClockDisplay").innerText = time;
      setTimeout(showTime, 1000);
  }

  showTime();
</script>
